public class App {
    public static void main(String[] args) throws Exception {

        for (int firstNameCount = 5; firstNameCount != 0; firstNameCount--) {
            System.out.println("Thea");
        }
        int surnameCount = 5;

        do {
            System.out.println("Nery");
            surnameCount--;
        } while (surnameCount != 0);
   
        
        int nickNameCount = 5;

        while (nickNameCount != 0) {
            System.out.println("thea");
            nickNameCount--;
        }
    }   

       
   
}
