public class App {
    public static void main(String[] args) throws Exception {

        for (int i = 5; i != 0; i--) {
            System.out.println("Thea");
        }
        int surnameCount = 5;

        do {
            System.out.println("Nery");
            surnameCount--;
        } while (surnameCount != 0);
   
        
        int nickName = 5;

        while (nickName != 0) {
            System.out.println("thea");
            nickName--;
        }
    }   

       
   
}
