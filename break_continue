public class App {
    public static void main(String[] args) throws Exception {


        //FOR LOOP
        for (int firstNameCount = 5; firstNameCount != 0; firstNameCount--) {

            if (firstNameCount == 1) {
                System.out.println("Timeout!");
                break;
            }
                

            System.out.println("Thea");
        }
        
        
        //DO-WHILE LOOP
        int surnameCount = 5;
        do {
            System.out.println("Nery");
            surnameCount--;
        } while (surnameCount != 0);
            
   
        //WHILE LOOP
        int nickNameCount = 5;
        while (nickNameCount != 0) {
            System.out.println("thea");
            nickNameCount--;
            
            if (nickNameCount == 1) {
                System.out.println("continue");
                continue;
            }

    
        }


    }
}
